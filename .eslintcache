h > 0 ? (\n          showAverage(product)\n        ) : (\n          <div className=\"text-center py-5\">\n            <h4>No Rating Yet</h4>\n          </div>\n        )}\n        <ProductListItems product={product} />\n        <h3 className=\"text-center pt-3\">Description</h3>\n        <div\n          className=\"col-12\"\n          dangerouslySetInnerHTML={{ __html: description }}\n        ></div>\n\n        <Card\n          actions={[\n            <>\n              <InfoOutlined className=\"text-success\" /> <br />\n              How to Download\n            </>,\n            <a href={downloadLink}>\n              <DownloadOutlined className=\"text-info\" /> <br /> Download\n            </a>,\n            <RatingModal>\n              <StarRating\n                name={_id}\n                numberOfStars={5}\n                rating={star}\n                changeRating={onStarClick}\n                isSelectable={true}\n                starRatedColor=\"red\"\n              />\n            </RatingModal>,\n          ]}\n        ></Card>\n      </div>\n    </div>\n  );\n};\n\nexport default SingleProduct;\n","/home/devro/Desktop/Projects/CMS/client/src/components/forms/LocalSearch.js",[],"/home/devro/Desktop/Projects/CMS/client/src/components/forms/ProductCreateForm.js",["257","258"],"import React from \"react\";\nimport { Select } from \"antd\";\nimport ReactQuill from \"react-quill\";\nimport \"react-quill/dist/quill.snow.css\";\n\nconst { Option } = Select;\n\nconst ProductCreateForm = ({\n  handleSubmit,\n  handleChange,\n  setValues,\n  values,\n  handleCatagoryChange,\n  subOptions,\n  showSub,\n}) => {\n  // destructure\n  const {\n    title,\n    description,\n    //price,\n    categories,\n    category,\n    subs,\n    downloadLink,\n    size,\n    //shipping,\n    //quantity,\n    images,\n    developer,\n    version,\n    //colors,\n    //brands,\n    //color,\n    //brand,\n  } = values;\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <div class